<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#343434" />
      <stroke
          android:width="1dp"
          android:color="#171717" />
      <corners
          android:radius="2dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#666666"
          android:endColor="#343434"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#343434" />
      <corners
          android:radius="2dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>