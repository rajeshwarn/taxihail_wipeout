@model List<apcurium.MK.Web.Areas.AdminTH.Controllers.PromoCode>

if (Model.Count > 0)
{
    <table class="table table-striped">
        @foreach (var promoCode in Model)
        {
            <tr>
                <td>@promoCode.Name</td>
                <td>@Url.Action("Edit")</td>
            </tr>
        }
    </table>
}
else
{

}