<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:local="http://schemas.android.com/apk/res/com.apcurium.MK.TaxiHailDemo"
    android:layout_height="fill_parent"
    android:layout_width="fill_parent">
    <RelativeLayout
        android:id="@+id/fakeLayout"
        android:layout_marginLeft="100px"
        android:layout_width="0dip"
        android:layout_height="fill_parent" />
    <apcurium.mk.booking.mobile.client.controls.HeaderedLayout
        android:id="@+id/HeaderLayoutMenu"
        local:HideLogo="true"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_toRightOf="@id/fakeLayout">
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
            <LinearLayout
                android:id="@+id/mainSettingsLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_centerHorizontal="true"
                android:layout_margin="10dip"
                android:layout_toRightOf="@id/fakeLayout">
                <Button
                    android:id="@+id/settingsFavorites"
                    style="@style/BigTitleTextBlack"
                    android:background="@drawable/cell_top_button_state"
                    android:layout_width="fill_parent"
                    android:layout_height="40dip"
                    android:text="@string/View_Book_Menu_MyLocations"
                    android:drawableRight="@drawable/right_arrow"
                    android:paddingRight="10dip"
                    android:paddingLeft="10dip"
                    android:gravity="left|center_vertical" />
                <Button
                    android:id="@+id/settingsHistory"
                    style="@style/BigTitleTextBlack"
                    android:background="@drawable/cell_middle_button_state"
                    android:layout_width="fill_parent"
                    android:layout_height="40dip"
                    android:text="@string/View_Book_Menu_MyOrders"
                    android:drawableRight="@drawable/right_arrow"
                    android:paddingRight="10dip"
                    android:paddingLeft="10dip"
                    android:gravity="left|center_vertical"
                    local:MvxBind="{&apos;Click&apos;:{&apos;Path&apos;:&apos;Panel.NavigateToOrderHistory&apos;}}" />
                <Button
                    android:id="@+id/settingsProfile"
                    local:MvxBind="{&apos;Click&apos;:{&apos;Path&apos;:&apos;Panel.NavigateToUpdateProfile&apos;}}"
                    style="@style/BigTitleTextBlack"
                    android:background="@drawable/cell_middle_button_state"
                    android:layout_width="fill_parent"
                    android:layout_height="40dip"
                    android:text="@string/View_Book_Menu_UpdateMyProfile"
                    android:drawableRight="@drawable/right_arrow"
                    android:paddingRight="10dip"
                    android:paddingLeft="10dip"
                    android:gravity="left|center_vertical" />
                <Button
                    android:id="@+id/settingsCallCompany"
                    style="@style/BigTitleTextBlack"
                    local:MvxBind="{&apos;Click&apos;:{&apos;Path&apos;:&apos;Panel.Call&apos;}}"
                    android:background="@drawable/cell_middle_button_state"
                    android:layout_width="fill_parent"
                    android:layout_height="40dip"
                    android:text="@string/View_Book_Menu_CallDispatch"
                    android:drawableRight="@drawable/right_arrow"
                    android:paddingRight="10dip"
                    android:paddingLeft="10dip"
                    android:gravity="left|center_vertical" />
                <Button
                    android:id="@+id/settingsAbout"
                    style="@style/BigTitleTextBlack"
                    android:background="@drawable/cell_middle_button_state"
                    android:layout_width="fill_parent"
                    android:layout_height="40dip"
                    android:text="@string/View_Book_Menu_AboutUs"
                    android:drawableRight="@drawable/right_arrow"
                    android:paddingRight="10dip"
                    android:paddingLeft="10dip"
                    android:gravity="left|center_vertical" />
                <Button
                    android:id="@+id/settingsSupport"
                    style="@style/BigTitleTextBlack"
                    android:background="@drawable/cell_middle_button_state"
                    android:layout_width="fill_parent"
                    android:layout_height="40dip"
                    android:text="@string/View_Book_Menu_ReportProblem"
                    android:drawableRight="@drawable/right_arrow"
                    android:paddingRight="10dip"
                    android:paddingLeft="10dip"
                    android:gravity="left|center_vertical" />
                <Button
                    android:id="@+id/settingsLogout"
                    style="@style/BigTitleTextBlack"
                    android:background="@drawable/blank_bottom_button_state"
                    android:layout_width="fill_parent"
                    android:layout_height="40dip"
                    android:text="@string/View_Book_Menu_SignOut"
                    android:drawableRight="@drawable/right_arrow"
                    android:paddingRight="10dip"
                    android:paddingLeft="10dip"
                    android:gravity="left|center_vertical"
                    local:MvxBind="{&apos;Click&apos;:{&apos;Path&apos;:&apos;Panel.SignOut&apos;}}" />
            </LinearLayout>
            <RelativeLayout
                android:id="@+id/apcuriumLogoLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_toRightOf="@id/fakeLayout"
                android:layout_alignParentBottom="true">
                <ImageView
                    android:id="@+id/apcuriumLogo"
                    android:src="@drawable/apcuriumLogo"
                    android:layout_width="wrap_content"
                    android:scaleType="fitCenter"
                    android:adjustViewBounds="true"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dip"
                    android:layout_marginRight="20dip"
                    android:paddingBottom="10dip"
                    android:layout_centerHorizontal="true" />
                <TextView
                    android:text="v0.0.0"
                    local:MvxBind="{&apos;Text&apos;:{&apos;Path&apos;:&apos;Panel.Version&apos;}}"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/apcuriumLogo"
                    style="@style/FinePrint"
                    android:gravity="bottom|right"
                    android:layout_gravity="right" />
            </RelativeLayout>
        </RelativeLayout>
    </apcurium.mk.booking.mobile.client.controls.HeaderedLayout>
    <RelativeLayout
        android:id="@+id/DShadow"
        android:layout_width="5dip"
        android:layout_height="fill_parent"
        android:layout_toRightOf="@id/fakeLayout"
        android:background="@drawable/drop_shadow" />
</RelativeLayout>